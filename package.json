{
  "name": "@studiostarum/custom-vimeo-player",
  "version": "1.4.1",
  "description": "A modern, responsive video player implementation built on top of the Vimeo Player SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/custom-vimeo-player.css",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/custom-vimeo-player.css",
    "./dist/custom-vimeo-player.css": "./dist/custom-vimeo-player.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "npm run build"
  },
  "keywords": [
    "vimeo",
    "video",
    "player",
    "lightbox",
    "typescript"
  ],
  "author": "Studio Starum",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiostarum/custom-vimeo-player.git"
  },
  "bugs": {
    "url": "https://github.com/studiostarum/custom-vimeo-player/issues"
  },
  "homepage": "https://github.com/studiostarum/custom-vimeo-player#readme",
  "peerDependencies": {
    "@vimeo/player": "^2.26.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/vimeo__player": "^2.18.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@vimeo/player": "^2.20.1",
    "lucide": "^0.484.0"
  }
